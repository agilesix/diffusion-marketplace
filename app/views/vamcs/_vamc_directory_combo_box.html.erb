<label for="<%= vamc_selector %>" class="usa-label line-height-26">VAMC</label>
<div class="usa-combo-box margin-top-1 margin-bottom-2 desktop:margin-y-0">
  <select id="<%= vamc_selector %>" class="usa-select" name="<%= vamc_selector %>">
    <% @vamcs.each do |va| %>
      <option value=<%= va["id"] %>>
        <%= va["common_name"] %>
      </option>
    <% end %>
  </select>
</div>
