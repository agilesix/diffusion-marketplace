<div class="grid-container">
  <section id="visns-map" class="margin-bottom-3">
    <%= render partial: "shared/messages", locals: { small_text: false } %>
    <h1 class="font-sans-2xl margin-top-8 margin-bottom-2 line-height-46px">VISN directory</h1>
    <span class="line-height-26">Looking for a list of VAMCs?</span> <%= link_to 'Click here', vamcs_path, class: 'dm-external-link' %>
  </section>
  <section id="visn-cards-section">
    <% @visns.each do |visn| %>
      <%= link_to visn_path(visn) do %>
        <p>VISN <%= visn.number %></p>
        <p><%= get_adopted_practices_count(visn) %></p>
      <% end %>
    <% end %>
  </section>
</div>